;(function(){var D={};function detect(ua){var os=this.os={},android=ua.match(/(Android);?[\s\/]+([\d.]+)?/),ipad=ua.match(/(iPad).*OS\s([\d_]+)/),ipod=ua.match(/(iPod)(.*OS\s([\d_]+))?/),iphone= !ipad && ua.match(/(iPhone\sOS)\s([\d_]+)/);if(android){os.android=true;os.version=android[2]};if(iphone&&!ipod){os.ios=os.iphone=true;os.version=iphone[2].replace(/_/g, '.')};if(ipad){os.ios=os.ipad=true;os.version=ipad[2].replace(/_/g, '.')};}detect.call(D,navigator.userAgent);if(D.os.android){window.location.href="http://www.7k7k.com/m-android/"}if(D.os.iphone){window.location.href="http://www.7k7k.com/m-iphone/"}if(D.os.ipad){window.location.href="http://www.7k7k.com/m-ipad/"}})();
