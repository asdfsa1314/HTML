	        var json = fsjsons[0];
            json = json.replace(/\n/g,""); 
			var obj = eval('(' + json + ')');
            var fs = obj.fs;
			var ExpRevRate = fs.ExpRevRate;
            var EprIdx = ExpRevRate.indexOf('业绩基准');
			var ExpRevRatetext =ExpRevRate;
			var EprTxt ='';
            if(EprIdx == 0){
            	ExpRevRatetext = ExpRevRate.replace(/业绩基准/g,'')
            	EprTxt = "业绩基准";
            }else{
				
                EprTxt = "预期年化收益率";
            }
            var EprIdx = ExpRevRate.indexOf('上期参考收益率');
            if(EprIdx == 0){
            	ExpRevRatetext = ExpRevRate.replace(/上期参考收益率/g,'')
            	EprTxt = "上期参考收益率";
            }
		    var fsfundstr='<p class="t_left mt10 ml14 abc_Cby_t">';
			fsfundstr+='<a title='+fs.ProdName+' href="'+fsurl+fs.ProdCode+'.htm" target="_blank">'+fs.ProdName.substring(0,15)+'...</a></p>'
									+'<p class="t_left abc_Cby_p"><span class="font_boldYellow font28 pc">'
											+ExpRevRatetext+'</span>'
											 +'<span class="font_gray">'+EprTxt+'</span>'
											 +'<span class="font_boldYellow font18 mobile_inline">'+ExpRevRatetext+'</span>'
									 +'</p>';
			$('#fsfund4').empty();
			$('#fsfund4').append(fsfundstr);
	   }else if(fsfundId4=='fundjsons0'){
